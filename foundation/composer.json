{
    "name": "confetti-cms/foundation",
    "description": "This is a package for the foundation of the Confetti CMS. For example, it contains the abstract ORM classes.",
    "keywords": [
        "Confetti",
        "CMS",
        "Blade"
    ],
    "homepage": "https://confetti-cms.com",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "files": [
        "fileloader.php"
    ],
    "autoload": {
        "psr-4": {
            "Confetti\\": "/var/resources/confetti-cms__parser/Confetti",
            "model\\": "/var/resources/confetti-cms__structure/model",
            "query\\": "/var/resources/confetti-cms__structure/query",
            "ConfettiCMS\\Foundation\\": "Src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConfettiCMS\\Foundation\\Tests\\": "Tests/"
        }
    }
}