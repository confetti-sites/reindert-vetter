{
  "require": {
    "eftec/bladeone": "^4.16",
    "colinodell/json5": "^3.0",
    "ext-curl": "*",
    "confetti-cms/foundation": "*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "foundation"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "/src/App/"
    },
    "files": [
      "/src/App/Confetti/Helpers/ComponentStandard.php",
      "/src/App/Confetti/Helpers/ContentStore.php",
      "/src/App/Confetti/Helpers/QueryBuilder.php",
      "/src/App/Confetti/Model/RawFile.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
