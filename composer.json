{
  "require": {
    "eftec/bladeone": "^4.16",
    "colinodell/json5": "^3.0",
    "ext-curl": "*",
    "confetti-cms/foundation": "*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "foundation"
    }
  ],
  "autoload": {
    "psr-4": {
      "Confetti\\Parser\\": "/var/resources/confetti-cms__parser/Confetti",
      "model\\": "/var/resources/confetti-cms__structure/model",
      "query\\": "/var/resources/confetti-cms__structure/query",
      "App\\": "/src/App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
